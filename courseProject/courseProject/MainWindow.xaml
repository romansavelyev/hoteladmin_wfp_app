<Window
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:courseProject"
        xmlns:CustomControls="clr-namespace:courseProject.CustomControls" x:Class="courseProject.MainWindow"
        mc:Ignorable="d"
        Title="MainWindow" Height="468" Width="757">
    <Grid>

        <Menu x:Name="HeaderMenu" Height="47" VerticalAlignment="Top" Background="Gray">
            <Button x:Name="SelectDB_Button" Background="{x:Null}" Height="41" Width="152" BorderBrush="{x:Null}" Foreground="WhiteSmoke" Content="Show DataBase" BorderThickness="0" Click="SelectDB_Button_Click"/>
            <Button x:Name="AddElement_Button" Background="{x:Null}" Height="41" Width="152" BorderBrush="{x:Null}" Foreground="WhiteSmoke" Content="Add element" BorderThickness="0" Click="AddElement_Button_Click"/>
            <Button x:Name="Delete_Button" Background="{x:Null}" Height="41" Width="152" BorderBrush="{x:Null}" Foreground="WhiteSmoke" Content="Delete" BorderThickness="0" Margin="0px" Click="Delete_Button_Click"/>
        </Menu>
        <ContentControl HorizontalAlignment="Left" Margin="11,52,0,0" VerticalAlignment="Top" Height="375" Width="729" x:Name="contentControl">
        </ContentControl>

    </Grid>
</Window>
